package com.thigpen.zooKeeper;

public class Mammal {
//	member variables
	public int energyLevel;
	
//constructor class
	public Mammal(int energyLevelInput) {
		this.energyLevel = energyLevelInput;
	}
	
	public int displayEnergy() {
		System.out.println("The mammal's energy level is " + this.energyLevel);
		return this.energyLevel;
	}
}
